{
    "name": "tdlog",
    "version": "0.1.0",
    "private": true,
    "proxy": "http://localhost:4000",
    "scripts": {
        "frontend": "cd frontend && npm start",
        "backend": "cd backend && npm start",
        "start": "npm run frontend",
        "start-with-api": "concurrently \"npm run frontend\" \"set PORT=4000 && nodemon ./backend/server/server.js\"",
        "build": "react-scripts build",
        "test": "react-scripts test --env=jsdom",
        "eject": "react-scripts eject"
    },
    "devDependencies": {
        "concurrently": "^3.5.0",
        "nodemon": "^1.12.1",
        "react-scripts": "1.0.14"
    },
    "dependencies": {
        "lodash": "^4.17.11",
        "react": "^16.0.0",
        "react-dom": "^16.0.0"
    }
}
